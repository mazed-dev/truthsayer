{
  "manifest_version": 3,
  "name": "Foreword",
  "author": "ASG Mazed Inc",
  "version": "comes-from-version.txt",
  "description": "Reference anything you've read, without looking for it. Foreword is your second brain, serving you information you've read before.",
  "homepage_url": "https://thinkforeword.com",
  "action": {
    "default_icon": {
      "16": "logo-16x16.png",
      "24": "logo-24x24.png",
      "72": "logo-72x72.png",
      "128": "logo-128x128.png"
    },
    "default_title": "Foreword",
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "logo-16x16.png",
      "24": "logo-24x24.png",
      "72": "logo-72x72.png",
      "128": "logo-128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {},
  "permissions": [
    "bookmarks",
    "contextMenus",
    "history",
    "tabs",
    "webNavigation",
    "storage",
    "unlimitedStorage"
  ],
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "file:///*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "externally_connectable": {
    "ids": [],
    "matches": []
  }
}
