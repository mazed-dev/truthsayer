{
  "manifest_version": 3,
  "name": "Mazed",
  "author": "Thread knowledge",
  "version": "0.0.1",
  "description": "Save your web to Mazed",
  "action": {
    "default_icon": {
      "16": "logo-16x16.png",
      "48": "logo-48x48.png",
      "72": "logo-72x72.png",
      "128": "logo-128x128.png",
      "240": "logo-240x240.png"
    },
    "default_title": "Mazed",
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "logo-16x16.png",
      "48": "logo-48x48.png",
      "72": "logo-72x72.png",
      "128": "logo-128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {},
  "permissions": [
    "activeTab",
    "contextMenus",
    "cookies",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.mazed.dev/",
    "http://localhost:3000/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
