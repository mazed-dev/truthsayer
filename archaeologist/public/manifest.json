{
  "manifest_version": 3,
  "name": "Mazed",
  "author": "Thread knowledge",
  "version": "0.0.1",
  "description": "Bookmark web pages to Mazed",
  "action": {
    "default_icon": {
      "16": "logo-16x16.png",
      "48": "logo-48x48.png",
      "72": "logo-72x72.png",
      "128": "logo-128x128.png",
      "240": "logo-240x240.png"
    },
    "default_title": "Mazed",
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "logo-16x16.png",
      "48": "logo-48x48.png",
      "72": "logo-72x72.png",
      "128": "logo-128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {},
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },
  "host_permissions": [
    ">>> This is set up in webpack config, depending on build options <<<"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "file:///*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
