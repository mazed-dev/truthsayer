{
  "name": "smuggler-api",
  "version": "0.0.1",
  "description": "Smuggler API artefacts in typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "author": "Thread knowledge",
  "license": "no",
  "private": true,
  "scripts": {
    "build": "$(yarn workspace thread-knowledge bin tsc) --pretty",
    "test": "$(yarn workspace thread-knowledge bin jest)",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "$(yarn workspace thread-knowledge bin eslint) \"./src/**/*.{js,ts}\"",
    "lint:prettier": "$(yarn workspace thread-knowledge bin prettier) --check \"./src/**/*.{css,md,js,json,ts}\"",
    "lint:typescript": "$(yarn workspace thread-knowledge bin tsc) --build ./ --force"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "query-string": "^7.0.0",
    "universal-cookie": "^4.0"
  }
}
