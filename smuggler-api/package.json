{
  "name": "smuggler-api",
  "description": "Smuggler API artefacts in typescript",
  "version": "0.0.1",
  "author": "Thread knowledge",
  "license": "no",
  "private": true,
  "scripts": {
    "build": "yarn tsc --build --pretty",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:typescript",
    "lint:eslint": "yarn eslint \"./src/**/*.{js,ts}\"",
    "lint:prettier": "yarn prettier --check \"./src/**/*.{css,md,js,json,ts}\"",
    "lint:typescript": "yarn tsc --build ./ --force",
    "postinstall": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "armoury": "0.0.1",
    "jest": "26.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "prettier": "^2.2",
    "query-string": "^7.0.0",
    "ts-jest": "^26.4.2",
    "typescript": "4.1.3",
    "universal-cookie": "^4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
