{
  "name": "text-information-retrieval",
  "description": "Text information retrieval",
  "author": "Mazed",
  "version": "0.0.1",
  "license": "no",
  "private": true,
  "scripts": {
    "build": "yarn tsc --build --pretty",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:typescript",
    "lint:eslint": "yarn eslint \"./src/**/*.{js,ts}\"",
    "lint:prettier": "yarn prettier --check \"./{src,test}/**/*.{css,md,js,json,ts}\"",
    "lint:typescript": "yarn tsc --build ./ --force",
    "postinstall": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "wink-eng-lite-web-model": "^1.4.3",
    "wink-nlp": "^1.13.0"
  },
  "devDependencies": {
    "@fast-csv/parse": "^4.3.6",
    "eslint": "^8.16.0",
    "jest": "26.6.0",
    "prettier": "^2.2",
    "ts-jest": "^26.4.2",
    "typescript": "4.1.3"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
