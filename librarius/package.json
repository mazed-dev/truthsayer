{
  "name": "librarius",
  "description": "Information extractors from variety of sources",
  "author": "Thread knowledge",
  "version": "0.0.1",
  "license": "no",
  "private": true,
  "scripts": {
    "build": "yarn tsc --build --pretty",
    "clean": "rm -r dist tsconfig.tsbuildinfo",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:typescript",
    "lint:eslint": "yarn eslint \"./src/**/*.{js,ts}\"",
    "lint:prettier": "yarn prettier --check \"./{src,test}/**/*.{css,md,js,json,ts}\"",
    "lint:typescript": "yarn tsc --build ./ --force",
    "postinstall": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "elementary": "0.0.1",
    "prettier": "^2.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-slate-transformer": "^0.6.7",
    "remark-stringify": "^10.0.2",
    "slate": "^0.81.0",
    "smuggler-api": "0.0.1",
    "typescript": "4.1.3",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@types/mdast": "^3.0.10",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "26.6.0",
    "ts-jest": "^26.4.2"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
