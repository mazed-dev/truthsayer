    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:typescript",
    "lint:eslint": "yarn eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prettier": "yarn prettier --check \"./src/**/*.{css,md,js,json,ts,tsx}\"",
    "lint:typescript": "yarn tsc --build ./ --force",
    "test": "yarn jest"
  },
  "dependencies": {
    "@emotion-icons/material": "^3.14.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "armoury": "0.0.1",
    "async-mutex": "^0.4.0",
    "eslint": "^8.16.0",
    "image-extensions": "^1.1.0",
    "is-url": "^1.2.4",
    "jsdom": "^17.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "posthog-js": "^1.53.4",
    "prettier": "^2.2",
    "react": "^16.14.0",
    "react-bootstrap": "^2.0.0",
    "react-dom": "^16.14.0",
    "remark-parse": "^10.0.1",
    "remark-slate": "^1.8.6",
    "slate": "^0.81.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.81.0",
