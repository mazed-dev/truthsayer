    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^12.6",
    "@types/is-url": "^1.2.30",
    "@types/jsdom": "^16.2.13",
    "@types/react-dom": "^16.9.16",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "26.6.0",
    "ts-jest": "^26.4.2"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
