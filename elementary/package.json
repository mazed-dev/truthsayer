{
  "name": "elementary",
  "description": "React shared elements",
  "author": "Thread knowledge",
  "version": "0.0.1",
  "license": "no",
  "private": true,
  "scripts": {
    "build": "yarn tsc --build --pretty",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:typescript",
    "lint:eslint": "yarn eslint \"./src/**/*.{js,ts}\"",
    "lint:prettier": "yarn prettier --check \"./src/**/*.{css,md,js,json,ts,tsx}\"",
    "lint:typescript": "yarn tsc --build ./ --force",
    "postinstall": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@testing-library/dom": "^7.29",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^12.6",
    "@types/is-url": "^1.2.30",
    "@types/jsdom": "^16.2.13",
    "eslint": "^7.11.0",
    "is-url": "^1.2.4",
    "jest": "26.6.0",
    "jsdom": "^17.0.0",
    "moment": "^2.29.1",
    "prettier": "^2.2",
    "slate": "^0.65.3",
    "slate-history": "^0.65.3",
    "slate-react": "^0.65.3",
    "ts-jest": "^26.4.2",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
