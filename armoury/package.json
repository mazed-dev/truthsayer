{
  "name": "armoury",
  "description": "TS/JS common helpers and utilities",
  "author": "Thread knowledge",
  "version": "0.0.1",
  "license": "no",
  "private": true,
  "scripts": {
    "build": "yarn tsc --build --pretty",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:typescript",
    "lint:eslint": "yarn eslint \"./src/**/*.{js,ts}\"",
    "lint:prettier": "yarn prettier --check \"./{src,test}/**/*.{css,md,js,json,ts}\"",
    "lint:typescript": "yarn tsc --build ./ --force",
    "postinstall": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "eslint": "^8.16.0",
    "jest": "26.6.0",
    "js-base64": "^3.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "posthog-js": "^1.34.0",
    "prettier": "^2.2",
    "query-string": "^7.0.0",
    "ts-jest": "^26.4.2",
    "typescript": "4.1.3",
    "uuid": "^8.3.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/xxhashjs": "^0.2.2"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
